{
    "name": "bennet0496/nextcloud_attachments",
    "license": "MIT",
    "description": "Upload large attachments to Nextcloud and automatically create share link",
    "type": "roundcube-plugin",
    "authors": [
        {
            "name": "Bennet Becker",
            "email": "dev@bennet.cc"
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://plugins.roundcube.net"
        }
    ],
    "require": {
        "php": ">=8.1",
        "roundcube/plugin-installer": "^0.3",
        "guzzlehttp/guzzle": "^7.8",
        "ext-simplexml": "*",
        "ext-json": "*"
    },
    "extra": {
        "roundcube": {
            "min-version": "1.6.1"
        }
    },
    "config": {
        "allow-plugins": {
            "roundcube/plugin-installer": true
        }
    }
}
